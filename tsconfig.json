{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2021",
		"outDir": "out",
		"lib": [
			"ES2021"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"alwaysStrict": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noUncheckedIndexedAccess": true,
		"useUnknownInCatchVariables": true,
	}
}