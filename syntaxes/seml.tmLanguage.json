{
    "name": "seml",
    "patterns": [
        {
            "name": "comment.seml",
            "match": "#.*$"
        },
        {
            "name": "",
            "match": "^(SET|J|G|C|C_POS|C_NUM|P[1-8]?|B[1-8]?|D[1-8]?|PP[1-8]?|DD[1-8]?|p[1-8]?|b[1-8]?|d[1-8]?|pp[1-8]?|dd[1-8]?|bb[1-8]?)\\s+(\\+?\\d+)?",
            "captures": {
                "1": {
                    "name": "entity.other.attribute-name.seml"
                },
                "2": {
                    "name": "constant.numeric.seml"
                }
            }
        },
        {
            "name": "keyword.seml",
            "match": "^w[1-9][\\s~0-9]*(?=$|#)"
        },
        {
            "name": "keyword.seml",
            "match": ":"
        },
        {
            "name": "entity.other.attribute-name.seml",
            "match": "\\b([a-zA-Z_]+)(?=:)"
        }
    ],
    "repository": {},
    "scopeName": "source.seml"
}