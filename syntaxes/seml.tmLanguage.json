{
    "name": "seml",
    "patterns": [
        {
            "name": "comment.seml",
            "match": "#.*$"
        },
        {
            "name": "",
            "match": "^(SET|G|A|J|a|A_NUM|J_NUM|a_NUM|C|C_POS|C_NUM|P[1-8]?|B[1-8]?|D[1-8]?|PP[1-8]?|DD[1-8]?|p[1-8]?|b[1-8]?|d[1-8]?|pp[1-8]?|dd[1-8]?|bb[1-8]?)\\s+(\\+?\\d+)?",
            "captures": {
                "1": {
                    "name": "entity.other.attribute-name.seml"
                },
                "2": {
                    "name": "constant.numeric.seml"
                }
            }
        },
        {
            "name": "keyword.seml",
            "match": "^w(?!0|~)[\\s~0-9]*(?=$|#)"
        },
        {
            "name": "keyword.seml",
            "match": ":"
        },
        {
            "name": "entity.other.attribute-name.seml",
            "match": "\\b([a-zA-Z_]+)(?=:)"
        },
        {
            "name": "constant.numeric.seml",
            "match": "障|杆|桶|报|门|橄|舞|潜|车|豚|丑|气|矿|跳|偷|梯|篮|白|红"
        },
        {
            "name": "constant.numeric.seml",
            "match": "\\b(cone|pole|buck|news|scre|foot|danc|snor|zomb|dolp|jack|ball|digg|pogo|bung|ladd|cata|garg|giga)\\b"
        },
        {
            "name": "keyword.seml",
            "match": "\\btrue|false\\b"
        }
    ],
    "repository": {},
    "scopeName": "source.seml"
}